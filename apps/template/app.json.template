        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "__PACKAGE_NAME__",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-sqlite"],
    "scheme": "__APP_SLUG__",
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
